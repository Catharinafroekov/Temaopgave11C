---
import BaseLayout from "../layout/BaseLayout.astro";
import { Image } from "astro:assets";
import EmployeeSection from "../components/EmployeeSection.astro";
import Header from "../components/Header.astro";
import Newsletter from "../components/Newsletter.astro";
import Footer from "../components/Footer.astro";

const employee = await fetch("https://raw.githubusercontent.com/Catharinafroekov/frontend-design/main/resources/json/employees.json")
.then(res => res.json());
---
<!-- <Image alt="" src={} format="avif"/> -->
<Header/>
<BaseLayout title="Team">
<EmployeeSection data={employee}/>
<!-- <EmployeeSection data={employee} theme="dark"/> -->
</BaseLayout>
<Newsletter/>
<Footer/>